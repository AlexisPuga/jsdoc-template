<?js

var config = env.config;
var customConfig = config['@custom'];
var themes = customConfig.themes;
var prettifyTheme = themes.prettify;
var siteName = customConfig.siteName;

var metadata = customConfig.metadata;
var metadataTags = metadata.tags;

var body = customConfig.body;
var bodyTags = body.tags;

var footer = customConfig.footer;
var footerTags = footer.tags;

var currentMember = /\:(.+)/.test(title) ? RegExp.$1 : RegExp.$0;

/** @return {string} */
var printTags = function (tags) {
	if (!Array.isArray(tags)) {
		tags = [tags];
	}

	return tags.join('\n\t');
};

?>
