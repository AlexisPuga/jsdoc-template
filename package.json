{
  "name": "@alexispuga/jsdoc-template",
  "version": "1.0.1-rc.10",
  "description": "A decent template for JSDoc.",
  "main": "publish.js",
  "keywords": [
    "JSDoc",
    "template",
    "redesign"
  ],
  "homepage": "https://github.com/AlexisPuga/jsdoc-template",
  "bugs": "https://github.com/AlexisPuga/jsdoc-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexisPuga/jsdoc-template.git"
  },
  "files": [
    "static/**/main.*",
    "static/**/*.min.*",
    "tmpl/**/*",
    "config.js"
  ],
  "author": "Alexis Puga Ru√≠z",
  "license": "Apache-2.0",
  "scripts": {
    "start": "NODE_ENV=test webpack-dev-server --config webpack.test.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "development": "NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "lint": "eslint . --cache",
    "test": "jsdoc docs/examples -t templates/silent -a all --pedantic",
    "pretest": "npm run lint",
    "precommit": "npm run test",
    "prepack": "npm run precommit && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy": "^0.3.2",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.2.1",
    "jsdoc": "^3.6.3",
    "jsdoc-webpack-plugin": "git://github.com/AlexisPuga/jsdoc-webpack-plugin.git#release-0.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "taffydb": "^2.7.3"
  }
}
